{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "vuestic.config.ts",
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "ignoreDeprecations": "5.0",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}
